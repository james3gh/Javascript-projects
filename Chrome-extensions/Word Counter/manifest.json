{
  "manifest_version": 2,
  "name": "Word Counter",
  "version": "1.0.0",
  "description": "counts the number of times a word appeared on the browser",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "count words"
  }
}
